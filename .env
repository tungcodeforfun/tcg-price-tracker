# Development Environment Configuration for TCG Price Tracker

# Application Settings
APP_DEBUG=true
APP_RELOAD=true
APP_LOG_LEVEL="DEBUG"
APP_HOST="0.0.0.0"
APP_PORT=8000

# Security Settings (development only)
SECURITY_SECRET_KEY="dev_secret_key_change_in_production_must_be_32_chars_long_for_testing"

# Database Settings (will be set via Docker Compose)
DB_HOST="localhost"
DB_PORT=5432
DB_NAME="tcgtracker"
DB_USER="tcgtracker"
DB_PASSWORD="tcgtracker_dev_password"

# Redis Settings (will be set via Docker Compose)
REDIS_HOST="localhost"
REDIS_PORT=6379

# Celery Settings (will be set via Docker Compose)
CELERY_BROKER_URL="redis://localhost:6379/1"
CELERY_RESULT_BACKEND="redis://localhost:6379/2"

# API Keys (placeholder values)
API_TCGPLAYER_CLIENT_ID="placeholder_tcgplayer_client_id"
API_TCGPLAYER_CLIENT_SECRET="placeholder_tcgplayer_client_secret"
API_TCGPLAYER_AUTH_CODE="placeholder_tcgplayer_auth_code"
API_EBAY_CLIENT_ID="placeholder_ebay_client_id"
API_EBAY_CLIENT_SECRET="placeholder_ebay_client_secret"